{
	"name": "@triplett/stew",
	"version": "3.0.0",
	"description": "A stateful virtual DOM for any purpose.",
	"author": "triplett",
	"license": "MIT",
	"main": "dist/stew.min.js",
	"scripts": {
		"test": "jest --watch",
		"build": "rimraf dist && esbuild src/index.js --outfile=dist/stew.min.js --bundle --minify --banner:js=//@triplett/stew@v3.0.0 --legal-comments=linked",
		"build:watch": "esbuild src/index.js --outfile=preview/stew.min.js --bundle --watch",
		"start": "node preview/server.js"
	},
	"keywords": [
		"state",
		"view",
		"declarative",
		"extensible"
	],
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/codetriplett/stew.git"
	},
	"bugs": {
		"url": "https://github.com/codetriplett/stew/issues"
	},
	"homepage": "https://github.com/codetriplett/stew#readme",
	"jest": {
		"testURL": "http://localhost",
		"setupFilesAfterEnv": [
			"./setupTests.js"
		]
	},
	"devDependencies": {
		"esbuild": "0.17.10",
		"jest": "26.6.3",
		"regenerator-runtime": "0.13.11",
		"rimraf": "3.0.2"
	}
}
